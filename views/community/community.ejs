</head>

<body>
  <!-- NAVBAR TOP -->
  <div id="top-navbar-placeholder"></div>
  <div id="gardens">
    <h2>Choose a garden</h2>
    <div class="gardenGroup">garden 1</div>
    <div class="gardenGroup">garden 2</div>
    <div class="gardenGroup">garden 3</div>
    <div class="gardenGroup">garden 4</div>
  </div>
  <div id="posts">
    <h2>Posts from all garden</h2>
    <!-- BOOTSTRAP EXAMPLE USED FOR POSTS -->

    <div>post 1</div>
    <div>post 2</div>
    <div>post 3</div>
    <div>post 4</div>
  </div>
  <footer></footer>

  <div>
    <h2>New Post</h2>
    
    <form action="posts" method="post" enctype="multipart/form-data">
      <input type="file" id="photo" name="photo">
      <input type="text" name="keyword" placeholder="photo">
      <input type="text" name="description" placeholder="desc">
      <input type="text" name="garden" placeholder="name">
      <button type="submit">Submit</button>
    </form>
  </div>
  
  







  <!-- NAVBAR BOTTOM -->
